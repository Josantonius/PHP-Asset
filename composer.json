{
    "name":        "josantonius/asset",
    "version":     "1.1.1",
    "type":        "library",
    "description": "PHP library for save CSS and JS files to be displayed in same place.",
    "keywords": [
        "Asset",
        "JS",
        "CSS",
        "HHVM",
        "Load resources",
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "info@josantonius.com",
            "homepage": "https://github.com/josantonius",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php" : ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Josantonius\\Asset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josantonius\\Asset\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
